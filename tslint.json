{
  "compilerOptions": {
        "noStrictGenericChecks": true
  },
  "rules": {
    "callable-types": true,
    "class-name": true,
    "no-construct": true,
    "no-empty-interface": true,
    "no-shadowed-variable": true,
    "no-string-throw": true,
    "no-switch-case-fall-through": true,
    "no-duplicate-variable": true
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
